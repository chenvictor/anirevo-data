<GridPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cvic.anirevo.editor.tabs.TabEventsContentController">
  <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="70.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Title" />
      <Label text="Location" GridPane.rowIndex="1" GridPane.rowSpan="2" />
      <Label text="Description" GridPane.rowIndex="5" />
      <TextField fx:id="textFieldTitle" GridPane.columnIndex="1" />
      <TextField fx:id="textFieldLocation" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextArea fx:id="textAreaDescription" prefHeight="150.0" prefWidth="400.0" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Label text="Age Restrict" GridPane.rowIndex="4" />
      <ChoiceBox fx:id="choiceAge" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Label text="TimeBlocks" GridPane.rowIndex="3" />
      <ListView fx:id="listViewTimeBlocks" maxHeight="50.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <ChoiceBox fx:id="choiceBoxLocation" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
</GridPane>
